{
  "name": "reactjs-basics",
  "version": "1.0.0",
  "description": "Basics of ReacJS",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "serve": "webpack-dev-server --content-base dist/ --inline --hot",
    "build": "webpack -d && cp src/index.html dist/index.html && npm run serve",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "author": "Raul Rama",
  "license": "MIT",
  "dependencies": {
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0"
  }
}
